<% for (let i = 0; i < sessions.length; i++) { %>
<div class="col">
  <a href="/sessions/<%= sessions[i].id %>" class="card h-100 text-decoration-none">
    <div class="card-body">
      <h5 class="card-title"><%= sessions[i].sport.name %> session #<%= sessions[i].id %></h5>
      <div class="d-flex align-items-center mb-3">
        <i class="bi bi-calendar3-fill me-2"></i>
        <p class="card-text mb-0"><%= sessions[i].date %></p>
      </div>
      <div class="d-flex align-items-center mb-3">
        <i class="bi bi-clock-fill me-2"></i>
        <p class="card-text mb-0"><%= sessions[i].time %></p>
      </div>
      <div class="d-flex align-items-center">
        <i class="bi bi-geo-alt-fill me-2"></i>
        <p class="card-text mb-0"><%= sessions[i].venue %></p>
      </div>
      <div class="d-flex align-items-center mt-3">
        <i class="bi bi-people-fill me-2"></i>
        <p class="card-text mb-0"><%= sessions[i].players.length %> filled out of <%= sessions[i].playerCount %></p>
      </div>
    </div>
  </a>
</div>
<% } %>